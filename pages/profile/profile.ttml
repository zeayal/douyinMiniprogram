    <!-- 用户信息部分 -->
    <view class="user-info-card">
      <view class="user-header">
        <!-- 用户头像 -->
        <image src="https://icons.unistar.icu/icons/user.png" class="user-icon"></image>
        <!-- 用户名 -->
        <view class="user-info">
          <text class="user-name">{{userInfo.nickname || '露营者'}}</text>
          <text class="user-vip">{{ userInfo.nickname ? '普通用户' : ''}}</text>
