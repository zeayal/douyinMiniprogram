    </view>
    <!-- 功能图标区域 -->
    <view class="function-grid">
      <view class="grid-row">
        <view class="grid-item {{!userInfo.isVip ? 'disabled' : ''}}" bindtap="handleMenuTap" data-type="collection">
          <view class="icon-wrapper">
            <image class="func-icon" src="https://icons.unistar.icu/icons/collect.png"></image>
          </view>
          <text class="func-name">我的收藏</text>
        </view>
        <view class="grid-item {{!userInfo.isVip ? 'disabled' : ''}}" bindtap="handleMenuTap" data-type="checkin">
          <view class="icon-wrapper">
            <image class="func-icon" src="https://icons.unistar.icu/icons/like.png"></image>
          </view>
          <text class="func-name">我的打卡</text>
        </view>
        <view class="grid-item {{!userInfo.isVip ? 'disabled' : ''}}" bindtap="handleMenuTap" data-type="camp">
          <view class="icon-wrapper">
            <image class="func-icon" src="https://icons.unistar.icu/icons/camp.png"></image>
          </view>
          <text class="func-name">我的营地</text>
        </view>
        <view class="grid-item" bindtap="handleMenuTap" data-type="addCamp">
          <view class="icon-wrapper">
            <image class="func-icon" src="https://icons.unistar.icu/icons/add.png"></image>
          </view>
          <text class="func-name">新增营地</text>
        </view>
      </view>
    </view>
